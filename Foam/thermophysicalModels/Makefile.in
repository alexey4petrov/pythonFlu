#--------------------------------------------------------------------------------------
pythonflu_root_dir:=@abs_top_builddir@

include @abs_top_builddir@/Foam/foam.version.makefile


#--------------------------------------------------------------------------------------
__CPPFLAGS__ := $(__CPPFLAGS__) \
	@OPENFOAM_FINITEVOLUME_CPPFLAGS@ \
	@OPENFOAM_BASICTHERMOPHYSICALMODELS_CPPFLAGS@ \
	@OPENFOAM_SPECIE_CPPFLAGS@ \
        @OPENFOAM_BASICSOLIDTHERMO_CPPFLAGS@ \
	@MANAGEDFLU_OPENFOAM_CPPFLAGS@ \
	@MANAGEDFLU_FINITEVOLUME_CPPFLAGS@ \
        @MANAGEDFLU_BASICTHERMOPHYSICALMODELS_CPPFLAGS@	\
        @MANAGEDFLU_BASICSOLIDTHERMO_CPPFLAGS@ \


#--------------------------------------------------------------------------------------
__LDFLAGS__ := $(__LDFLAGS__) \
	@OPENFOAM_FINITEVOLUME_LIBS@ \
	@OPENFOAM_BASICTHERMOPHYSICALMODELS_LIBS@ \
	@OPENFOAM_SPECIE_LIBS@ \
        @OPENFOAM_BASICSOLIDTHERMO_LIBS@ \
	@MANAGEDFLU_OPENFOAM_LIB@ \
	@MANAGEDFLU_FINITEVOLUME_LIB@ \
        @MANAGEDFLU_BASICTHERMOPHYSICALMODELS_LIB@ \
        @MANAGEDFLU_BASICSOLIDTHERMO_LIB@ \


#--------------------------------------------------------------------------------------
sources = \
	@abs_top_builddir@/Foam/src/OpenFOAM/fields/tmp/autoPtr_basicThermo.cxx \
	@abs_top_builddir@/Foam/src/OpenFOAM/containers/Lists/PtrList/PtrList_basicThermo.cxx \
	\
	@abs_top_builddir@/Foam/src/OpenFOAM/fields/tmp/autoPtr_basicPsiThermo.cxx \
	@abs_top_builddir@/Foam/src/OpenFOAM/containers/Lists/PtrList/PtrList_basicPsiThermo.cxx \
	\
	@abs_top_builddir@/Foam/src/OpenFOAM/fields/tmp/autoPtr_basicRhoThermo.cxx \
	@abs_top_builddir@/Foam/src/OpenFOAM/containers/Lists/PtrList/PtrList_basicRhoThermo.cxx \
	\
	@abs_top_builddir@/Foam/src/OpenFOAM/fields/tmp/autoPtr_basicSolidThermo.cxx 


#--------------------------------------------------------------------------------------
include @abs_top_builddir@/Foam/include.foam.makefile


#--------------------------------------------------------------------------------------
